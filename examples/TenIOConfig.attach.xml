<?xml version="1.0" encoding="UTF-8"?>
<Server>

	<Properties>
		<Property name="name">tenio-examples</Property>
		<Property name="id">tenio-examples</Property>
		<Property name="versionCode">1</Property>
		<Property name="versionName">1.0</Property>
	</Properties>

	<Network>
		<Sockets>
			<Port name="main" type="tcp">8032</Port> <!-- The first element is always treated the main connection -->
			<Port name="move" type="udp">8031</Port>
		</Sockets>
		<WebSockets>
		</WebSockets>
		<Http>
			<Port name="main" value="9999">
				<Path name="get" desc="retrieve a value" version="1"
					method="get">/retrieve/*</Path>
				<Path name="add" desc="add a new value" version="1"
					method="post">/add/*</Path>
				<Path name="change" desc="change a current value" version="1"
					method="put">/change/*</Path>
				<Path name="delete" desc="delete a existing value" version="1"
					method="delete">/delete/*</Path>
			</Port>
		</Http>
	</Network>

	<Configuration>
		<Properties>
			<Property name="producerThreadsPoolSize">1</Property>
			<Property name="consumerThreadsPoolSize">2</Property>
			<Property name="keepPlayerOnDisconnect">false</Property>
			<Property name="maxNumberPlayers">1000</Property>
			<Property name="idleReaderTime">180</Property>		<!-- Unit: Seconds -->
			<Property name="idleWriterTime">60</Property>		<!-- Unit: Seconds -->
			<Property name="emptyRoomScanInterval">30</Property>	<!-- Unit: Seconds -->
			<Property name="timeoutScanInterval">45</Property>		<!-- Unit: Seconds -->
			<Property name="ccuScanInterval">60</Property>			<!-- Unit: Seconds -->
			<Property name="deadlockScanInterval">60</Property>			<!-- Unit: Seconds -->
			<Property name="trafficCounterCheckInterval">60</Property>	<!-- Unit: Seconds -->
			<Property name="systemMonitoringInterval">60</Property>		<!-- Unit: Seconds -->
		</Properties>
	</Configuration>

	<Extension>
		<Properties>
			<Property name="customValue1">String</Property>
			<Property name="customValue2">1</Property>
			<Property name="customValue3">1.5</Property>
			<Property name="customValue4">True</Property>
		</Properties>
	</Extension>

</Server>