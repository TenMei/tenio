<?xml version="1.0" encoding="UTF-8"?>
<Server>

	<Properties>
		<Property name="name">tenio-examples</Property>
		<Property name="id">tenio-examples</Property>
		<Property name="versionCode">1</Property>
		<Property name="versionName">1.0</Property>
	</Properties>

	<Network>
		<Sockets>
			<Port name="main" type="tcp">8032</Port> <!-- The first element is always treated the main connection -->
		</Sockets>
		<Broadcasts>
			<ServerPort>9999</ServerPort>
			<Port id="move" name="move">8040</Port>
		</Broadcasts>
		<WebSockets>
		</WebSockets>
	</Network>

	<Configuration>
		<Workers>
			<Worker name="socketThreadsPoolAcceptor">10</Worker>
			<Worker name="socketThreadsPoolWorker">50</Worker>
			<Worker name="datagramThreadsPoolWorker">1</Worker>
			<Worker name="websocketThreadsPoolAcceptor">1</Worker>
			<Worker name="websocketThreadsPoolWorker">1</Worker>
		</Workers>
		<Schedules>
			<Task name="emptyRoomScanInterval">30</Task>	<!-- Unit: Seconds -->
			<Task name="timeoutScanInterval">45</Task>		<!-- Unit: Seconds -->
			<Task name="ccuScanInterval">60</Task>			<!-- Unit: Seconds -->
			<Task name="deadlockScanInterval">60</Task>			<!-- Unit: Seconds -->
			<Task name="trafficCounterCheckInterval">60</Task>	<!-- Unit: Seconds -->
			<Task name="systemMonitoringInterval">60</Task>		<!-- Unit: Seconds -->
		</Schedules>
		<Properties>
			<Property name="keepPlayerOnDisconnect">false</Property>
			<Property name="maxNumberPlayers">1000</Property>
			<Property name="idleReaderTime">180</Property>		<!-- Unit: Seconds -->
			<Property name="idleWriterTime">60</Property>		<!-- Unit: Seconds -->
		</Properties>
	</Configuration>

	<Extension>
		<Properties>
			<Property name="customValue1">String</Property>
			<Property name="customValue2">1</Property>
			<Property name="customValue3">1.5</Property>
			<Property name="customValue4">True</Property>
		</Properties>
	</Extension>

</Server>